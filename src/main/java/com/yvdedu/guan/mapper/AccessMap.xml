<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yvdedu.guan.mapper.AccessMap">
	<select id="find" resultType="String">
		 select r.name from test r where r.id=#{rid}
	</select>
	<select id="findAll" resultType="Access">
		 select * from test
	</select>
</mapper>